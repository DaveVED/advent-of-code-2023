{
  "name": "advent-of-code-2023",
  "version": "1.0.0",
  "description": "Advent of Code 2023 [typescript]",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "format:write": "prettier --ignore-path .gitignore --write src/**/*.ts",
    "format:check": "prettier --ignore-path .gitignore --check src/**/*.ts",
    "lint": "eslint --ignore-path .gitignore . src/**/*.ts",
    "ts-node": "ts-node",
    "test": "jest --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
